<asf xmlversion="1.0">
  <device-map id="map.atmel">
    <group name="avr">
      <group name="xmega">
        <group name="xmegaa">
          <group name="xmegaa1">
            <mcu name="atxmega128a1"/>
          </group>
          <group name="xmegaa3">
            <mcu name="atxmega63a3"/>
          </group>
          <group name="xmegaa4">
            <mcu name="atxmega32a4"/>
          </group>
        </group>
      </group>
      <group name="uc3">
        <group name="ucr2">
          <group name="uc3a">
            <group name="uc3a0">
              <mcu name="at32uc3a0512"/>
            </group>
          </group>
        </group>
      </group>
    </group>
  </device-map>

  <board id="avr32.boards.evk1100" caption="EVK1100 board support" mcu="at32uc3a0512">
    <build type="include-path" value="."/>
    <build type="c-source" value="init.c"/>
    <build type="c-source" value="led.c"/>
    <build type="define" name="BOARD" value="EVK1100"/>
    <build type="header-file" value="evk1100.h"/>
  </board>

  <module type="driver" id="avr32.driver.test" caption="Test driver">
    <build type="include-path" value="."/>
    <build type="c-source" value="init.c"/>
    <build type="c-source" value="led.c"/>
    <build type="header-file" value="evk1100.h"/>
    <require idref="basic"/>
    <device-support value="uc3a" />
    <device-support value="xmega" />
    <info type="keyword" value="testcategory">
      <keyword value="test1"/>
      <keyword value="test2"/>
    </info>
    <info type="license" value="unittest-license"/>
    <info type="appnote" value="AVR123"/>
    <info type="appnote" value="AVR234"/>
  </module>

  <module type="driver" id="basic" caption="Basic">
    <build type="include-path" value="."/>
    <build type="c-source" value="init.c"/>
    <build type="c-source" value="led.c"/>
    <build type="header-file" value="evk1100.h"/>
    <device-support value="uc3a" />
  </module>

  <project id="myproj" caption="My proj">
    <build type="include-path" value="."/>
    <build type="c-source" value="init.c"/>
    <build type="c-source" value="led.c"/>
    <build type="header-file" value="evk1100.h"/>
    <build type="define" name="MYPROJ" value="1" toolchain="verbose"/>
    <generator value="verbose"/>
    <generator value="doxygen"/>
    <require idref="basic"/>
    <require idref="avr32.driver.test"/>
    <require idref="avr32.boards.evk1100"/>
    <config name="cookiemonster" value="C is for cookie" />
    <config name="double.double" value="twice" />
    <config name="double.double" value="twice" />
    <config name="double" value="twice" />
    <config name="double" value="twice" />
  </project>

</asf>
