<asf xmlversion="1.0">
	<documentation baseurl="http://dummy/"/>
	<device-map id="map.atmel">
		<group name="avr">
			<group name="xmega">
				<group name="xmegaa">
					<group name="xmegaa1">
						<mcu name="atxmega128a1"/>
						<mcu name="atxmega256a3"/>
					</group>
					<group name="xmegaa4">
						<mcu name="atxmega32a4"/>
					</group>
				</group>
			</group>
			<group name="uc3">
				<group name="uc3l">
					<mcu name="at32uc3l0256"/>
				</group>
			</group>
		</group>
	</device-map>

	<project id="xmega.driver.example1.xplain" caption="Awesome driver example 1 for Xplain">
		<require idref="board.xplain"/>
		<require idref="xmega.driver.example1"/>
	</project>

	<board id="board.xplain" caption="Xplain board support" mcu="atxmega128a1">
	</board>

	<module type="application" id="xmega.driver.example1" caption="Awesome driver example 1">
		<device-support value="xmega"/>
		<require idref="xmega.driver.1"/>
	</module>

	<module type="driver" id="xmega.driver.1" caption="XMEGA driver">
		<device-support value="xmega"/>
	</module>

	<module type="component" id="xmega.driver.2" caption="XMEGA component">
		<device-support value="xmegaa"/>
		<require idref="xmega.meta"/>
	</module>

	<module type="service" id="xmega.meta" caption="XMEGA meta">
		<device-support value="xmegaa4"/>
	</module>

	<module type="service" id="xmega.driver.3" caption="XMEGA service">
		<device-support value="xmegaa1"/>
	</module>

	<module type="driver" id="xmega.driver.4" caption="XMEGA driver">
		<device-support value="xmegaa4"/>
	</module>

	<module type="driver" id="xmega.driver.5" caption="XMEGA driver">
		<device-support value="atxmega256a3"/>
	</module>

	<module type="driver" id="xmega.driver.hidden" caption="XMEGA driver">
		<device-support value="xmegaa"/>
		<info type="gui-flag" value="hidden" />
	</module>

	<module type="service" id="uc3.driver.1" caption="UC3 driver 1">
		<device-support value="uc3l"/>
	</module>

	<module type="service" id="uc3.driver.2" caption="UC3 driver 2">
		<device-support value="uc3l"/>
		<require idref="uc3.driver.hidden"/>
	</module>

	<module type="service" id="uc3.driver.hidden" caption="Hidden UC3 driver">
		<device-support value="uc3l"/>
		<info type="gui-flag" value="hidden" />
	</module>

	<module type="service" id="uc3.driver.3" caption="UC3 driver 3">
		<device-support value="uc3l"/>
	</module>

</asf>
