<asf xmlversion="1.0">
	<documentation baseurl="http://dummy/"/>

	<device-map id="map.atmel">
		<group name="avr">
			<group name="xmega">
				<group name="xmegaa">
					<group name="xmegaa1">
						<mcu name="atxmega128a1"/>
					</group>
				</group>
			</group>
		</group>
	</device-map>

	<project id="project" caption="project">
		<require idref="testapp" />
		<require idref="board" />
	</project>

	<project id="my_name_project" caption="My name project" workspace-name="my_name">
		<require idref="avr.drivers.some.module.example"/>
		<require idref="board"/>
	</project>

	<project id="gen_name_project_1" caption="Generator name project nr.1">
		<require idref="avr.drivers.some.module.example"/>
		<require idref="board"/>
	</project>

	<project id="gen_name_project_2" caption="Generator name project nr.2">
		<require idref="avr.applications.some.awesome.example"/>
		<require idref="board"/>
	</project>

	<project id="fail_name_project" caption="Failing generator name project">
		<require idref="fail.application"/>
		<require idref="board"/>
	</project>

	<board id="board" mcu="atxmega128a1" caption="board">
	</board>

	<module type="meta" id="appmeta1" caption="meta 1">
		<require idref="appmeta2"/>
	</module>

	<module type="meta" id="appmeta2" caption="meta 2">
		<require idref="driver2"/>
	</module>

	<module type="driver" id="driver1" caption="driver 1">
		<build type="header-file" subtype="api" value="driver1_api.h"/>
		<device-support value="avr"/>
	</module>

	<module type="driver" id="driver2" caption="driver 2">
		<build type="header-file" subtype="api" value="driver2_api.h"/>
		<build type="header-file" value="driver2_not_api.h"/>
		<device-support value="avr"/>
	</module>

	<module type="application" id="avr.drivers.some.module.example" caption="">
		<device-support value="avr"/>
	</module>

	<module type="application" id="avr.applications.some.awesome.example" caption="">
		<device-support value="avr"/>
	</module>

	<module type="application" id="fail.application" caption="">
		<device-support value="avr"/>
	</module>

	<select-by-device id="testapp" caption="app select by device">
		<module type="application" id="testapp#xmegaa" caption="app for xmegaa">
			<build type="header-file" value="xmega.h"/>
			<require idref="appmeta1"/>
			<require idref="driver1"/>
			<device-support value="xmegaa" />
		</module>
	</select-by-device>

</asf>
