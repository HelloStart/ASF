<asf xmlversion="1.0">
  <documentation baseurl="http://dummy/"/>

  <device-map id="map.atmel">
    <group name="avr">
      <group name="xmega">
        <group name="xmegaa">
          <group name="xmegaa1">
            <mcu name="atxmega128a1"/>
          </group>
          <group name="xmegaa3">
            <mcu name="atxmega63a3"/>
          </group>
          <group name="xmegaa4">
            <mcu name="atxmega32a4"/>
          </group>
        </group>
      </group>
      <group name="uc3">
        <group name="ucr2">
          <group name="uc3a">
            <group name="uc3a0">
              <mcu name="at32uc3a0512"/>
            </group>
          </group>
        </group>
      </group>
    </group>
  </device-map>

  <board id="avr32.boards.evk1100" caption="EVK1100 board support" mcu="at32uc3a0512">
    <build type="include-path" value="."/>
    <build type="c-source" value="init.c"/>
    <build type="c-source" value="led.c"/>
    <build type="define" name="BOARD" value="EVK1100"/>
    <build type="header-file" value="evk1100.h"/>
  </board>

  <board id="avr32.boards.evkxxyy" caption="EVKxxyy board support" mcu="at32uc3a0512">
    <build type="include-path" value="."/>
    <build type="c-source" value="init.c"/>
    <build type="c-source" value="led.c"/>
    <build type="define" name="BOARD" value="EVK1100"/>
    <build type="header-file" value="evk1100.h"/>
  </board>

  <module type="driver" id="avr32.driver.test" caption="Test driver">
    <build type="include-path" value="."/>
    <build type="c-source" value="init.c"/>
    <build type="c-source" value="led.c"/>
    <build type="header-file" value="evk1100.h"/>
    <require idref="basic"/>
    <device-support value="uc3a" />
    <device-support value="xmega" />
    <info type="keyword" value="testcategory">
      <keyword value="test1"/>
      <keyword value="test2"/>
    </info>
    <info type="appnote" value="AVR123"/>
    <info type="appnote" value="AVR234"/>
  </module>

  <module type="driver" id="basic" caption="Basic">
    <build type="include-path" value="."/>
    <build type="c-source" value="init.c"/>
    <build type="c-source" value="led.c"/>
    <build type="header-file" value="evk1100.h"/>
    <device-support value="uc3a" />
  </module>

  <project id="myproj" caption="My proj">
    <build type="include-path" value="."/>
    <build type="c-source" value="init.c"/>
    <build type="c-source" value="led.c"/>
    <build type="header-file" value="evk1100.h"/>
    <build type="define" name="MYPROJ" value="1" toolchain="verbose"/>
    <generator value="verbose"/>
    <generator value="doxygen"/>
    <require idref="basic"/>
    <require idref="avr32.driver.test"/>
    <require idref="avr32.boards.evk1100"/>
    <config name="cookiemonster" value="C is for cookie" />
  </project>

  <project id="statusproj1" type="unit-test" caption="status project 1">
    <require idref="basic"/>
    <require idref="avr32.boards.evk1100"/>
  </project>

  <project id="statusproj2" type="normal"  caption="status project 2">
    <require idref="avr32.driver.test"/>
    <require idref="avr32.boards.evkxxyy"/>
  </project>

  <project id="gen_name_proj" caption="Generator name project">
    <require idref="avr32.boards.evk1100"/>
    <require idref="name_app"/>
  </project>

  <project id="my_name_proj" caption="My name project" workspace-name="my_name">
    <require idref="avr32.boards.evk1100"/>
    <require idref="name_app"/>
  </project>

  <module type="application" id="name_app" caption="Project name app">
  </module>

  <module type="driver" id="devicesuppdriver_correct" caption="Device support 1">
    <build type="include-path" value="."/>
    <device-support value="atxmega128a1" />
    <require idref="devicesuppdriver2"/>
  </module>

  <module type="driver" id="devicesuppdriver_wrong" caption="Device support 1">
    <build type="include-path" value="."/>
    <device-support value="uc3" />
    <require idref="devicesuppdriver2"/>
  </module>

  <module type="driver" id="devicesuppdriver2" caption="Device support 2">
    <build type="include-path" value="."/>
    <require idref="devicesuppdriver3"/>
    <device-support value="xmega" />
  </module>

  <module type="driver" id="devicesuppdriver3" caption="Device support 3">
    <build type="include-path" value="."/>
    <device-support value="at32uc3a0512" />
  </module>

  <module type="driver" id="information" caption="Information">
    <build type="include-path" value="."/>
    <device-support value="at32uc3a0512" />
    <info type="description" value="summary">
	Summary
	</info>
    <info type="description" value="summary">
	Summary2
	</info>
    <info type="description" value="detailed">
	Detailed

	Detailed
	</info>

  </module>

</asf>
