<asf xmlversion="1.0">
  <documentation baseurl="http://dummy/"/>

  <device-map id="map.atmel">
    <group name="avr">
      <group name="xmega">
        <group name="xmegaa">
          <group name="xmegaa1">
            <mcu name="atxmega128a1"/>
          </group>
          <group name="xmegaa3">
            <mcu name="atxmega63a3"/>
          </group>
          <group name="xmegaa4">
            <mcu name="atxmega32a4"/>
          </group>
        </group>
      </group>
      <group name="uc3">
        <group name="ucr2">
          <group name="uc3a">
            <group name="uc3a0">
              <mcu name="at32uc3a0512"/>
            </group>
          </group>
        </group>
      </group>
    </group>
  </device-map>

  <board id="avr32.boards.evk1100" caption="Board support">
    <info type="gui-flag" value="visible"/>
    <device-support value="at32uc3a0512"/>
  </board>

  <module type="driver" id="avr32.driver.test" caption="Test driver">
    <require idref="basic"/>
    <info type="gui-flag" value="hidden"/>
  </module>

  <module type="driver" id="basic" caption="Basic">
  </module>

  <project id="project" caption="My proj">
    <require idref="basic"/>
    <require idref="avr32.driver.test"/>
    <require idref="avr32.boards.evk1100"/>
    <info type="gui-flag" value="hidden"/>
    <device-support value="at32uc3a0512"/>
  </project>

</asf>
