<asf xmlversion="1.0">
<documentation baseurl="http://dummy/"/>

<device-map id="map.atmel">
	<group name="avr">
		<group name="xmega">
			<group name="xmegaa">
				<group name="xmegaa1">
					<mcu name="atxmega128a1"/>
				</group>
			</group>
		</group>
		<group name="uc3">
			<group name="ucr2">
				<group name="uc3a">
					<group name="uc3a0">
						<mcu name="at32uc3a0512"/>
					</group>
				</group>
			</group>
		</group>
	</group>
</device-map>

<project id="project1" caption="my project">
	<config name="compiler.conf1" value="val1" />
	<config name="compiler.conf2" value="val2" />
	<config name="compiler.conf3" value="val3" />
	<config name="single1" value="singleval1" />
	<require idref="board1" />
	<device-support value="atxmega128a1"/>
</project>

<board id="board1" caption="my board">
	<device-support value="atxmega128a1"/>
</board>

<project id="project2" caption="my project">
	<config name="duplicate.same" value="val1" />
	<config name="duplicate.same" value="val2" />
	<config name="dup_single" value="val_project1" />
	<config name="dup_single" value="val_project1" />
	<require idref="board2" />
</project>

<board id="board2" caption="my board">
	<device-support value="atxmega128a1"/>
</board>

<project id="project3" caption="my project">
	<config name="compiler.conf1" value="val1" />
	<config name="compiler.conf2" value="val2" />
	<config name="conf1" value="val_project1" />
	<require idref="board3" />
	<require idref="driver" />
	<device-support value="atxmega128a1"/>
</project>

<board id="board3" caption="my board">
	<config name="conf1" value="val_board1" />
	<config name="conf2" value="val_board2" />
	<config name="compiler.conf2" value="board2" />
	<config name="compiler.conf3" value="board3" />
	<device-support value="atxmega128a1"/>
</board>

<module type="meta" id="driver" caption="mydriver">
</module>

</asf>
