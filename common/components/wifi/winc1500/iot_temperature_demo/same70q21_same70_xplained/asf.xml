<asf xmlversion="1.0">
	<project caption="WINC1500 IoT Temperature Sensor Demo for SAME70 XPlained" id="common.components.wifi.winc1500.iot_temperature_demo.same70_xplained">
		<!-- Summary Description -->
		<info type="description" value="summary">
			This IoT temperature sensor demo is configured to work with the SAM E70 Xplained using the WINC1500 Wi-Fi module.
		</info>

		<!-- Source -->
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_at30tse75x.h"/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_extint.h"/>
		<build type="header-file" value="conf_spi.h"/>
		<build type="header-file" value="conf_winc.h"/>
		<build type="header-file" value="conf_uart_serial.h"/>

		<!-- Device support -->
		<device-support value="atsame70q21"/>

		<!-- Required modules -->
		<require idref="common.components.wifi.winc1500.iot_temperature_demo"/>
		<require idref="board.same70_xplained"/>

		<!-- Generators -->
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<generator value="doxygen"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<config name="config.compiler.as5_arm.create_aux" value="no"/>
	</project>
</asf>
