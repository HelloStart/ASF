			<build type="include-path" value="firmware"/>
			<build type="distribute" subtype="file" value="firmware/winc1500_ota.bin"/>

			<!-- Device support -->
			<device-support value="sam4s"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc1500"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
		<module type="application" id="common.components.wifi.winc1500.ota_firmware_upgrade_example#2" caption="WINC1500 OTA Firmware Upgrade Example">
