<asf xmlversion="1.0">
	<select-by-device id="common.components.wifi.winc1500.ota_firmware_upgrade_example">
		<module type="application" id="common.components.wifi.winc1500.ota_firmware_upgrade_example#0" caption="WINC1500 OTA Firmware Upgrade Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates the use of the WINC1500 with the SAMD21 Xplained Pro board to perform an OTA Firmware upgrade.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="main_samd21.c"/>
			<build type="header-file" value="main.h"/>
			<build type="include-path" value="firmware"/>
			<build type="distribute" subtype="file" value="firmware/winc1500_ota.bin"/>

			<!-- Device support -->
			<device-support value="samd21"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc1500"/>
			<require idref="sam0.utils"/>
			<require idref="sam0.utils.stdio"/>
			<require idref="sam0.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
		<module type="application" id="common.components.wifi.winc1500.ota_firmware_upgrade_example#1" caption="WINC1500 OTA Firmware Upgrade Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
