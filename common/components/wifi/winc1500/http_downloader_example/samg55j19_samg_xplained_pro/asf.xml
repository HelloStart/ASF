<asf xmlversion="1.0">
	<project caption="WINC1500 HTTP Downloader Example for SAMG55 XPlained Pro" id="common.components.wifi.winc1500.http_downloader_example.samg55_xplained_pro">
		<!-- Summary Description -->
		<info type="description" value="summary">
			This example demonstrates how to retrieve a file from an HTTP web server using the WINC1500 Wi-Fi module.
		</info>

		<!-- Source -->
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_sleepmgr.h"/>
		<build type="header-file" value="conf_access.h"/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_fatfs.h"/>
		<build type="header-file" value="conf_sd_mmc.h"/>
		<build type="header-file" value="conf_spi_master.h"/>
		<build type="header-file" value="conf_sw_timer.h"/>
		<build type="header-file" value="conf_uart_serial.h"/>
		<build type="header-file" value="conf_winc.h"/>
		
		<config name="config.common.components.memory.sd_mmc" value="spi"/>
		<config name="config.common.components.memory.sd_mmc.ctrl_access" value="enable"/>

		<!-- Device support -->
		<device-support value="atsamg55j19"/>

		<!-- Required modules -->
		<require idref="common.components.wifi.winc1500.http_downloader_example"/>
		<require idref="board.samg55_xplained_pro"/>

		<!-- Generators -->
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<generator value="doxygen"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<config name="config.compiler.as5_arm.create_aux" value="no"/>
	</project>
</asf>
