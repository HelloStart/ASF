<asf xmlversion="1.0">
	<select-by-device id="common.components.wifi.winc3400.provision_ap_with_ble_example">
		<module type="application" id="common.components.wifi.winc3400.provision_ap_with_ble_example#0" caption="WINC3400 Provision AP with BLE Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates how to provision Wi-Fi via BLE using the WINC3400 Wi-Fi/BLE module.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="main21.c"/>
			<build type="header-file" value="main.h"/>
			<build type="distribute" subtype="file" value="android_app/Atmel_BLE_prov.apk"/>
			<build type="distribute" subtype="file" value="android_app/Atmel_Beacon_Radar.apk"/>

			<!-- Device support -->
			<device-support value="samd21"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc3400"/>
			<require idref="sam0.utils"/>
			<require idref="sam0.utils.stdio"/>
			<require idref="sam0.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
