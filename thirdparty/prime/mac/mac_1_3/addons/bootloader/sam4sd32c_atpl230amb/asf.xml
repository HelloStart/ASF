<asf xmlversion="1.0">
	<project caption="Bootloader for ATPL230AMB" id="thirdparty.prime.mac_1_3.addons.bootloader.atpl230amb">
		<info type="description" value="summary">
			Bootloader with FU management capability for ATPL230AMB.
		</info>
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_sleepmgr.h"/>
		<build type="header-file" value="conf_version.h"/>
		<require idref="thirdparty.prime.mac_1_3.addons.bootloader"/>
		<require idref="board.atpl230amb"/>
		<generator value="iarewarm"/>
		<generator value="as5_arm"/>
		<build type="linker-config" subtype="script" value="../linker_scripts/iar/atpl230amb_flash.icf" toolchain="iarewarm"/>
		<build type="linker-config" subtype="script" value="../linker_scripts/gcc/atpl230amb_flash.ld" toolchain="armgcc"/>
		<build type="linker-config" subtype="script" value="../linker_scripts/gcc/atpl230amb_flash.ld" toolchain="as5_arm"/>
		<config name="config.compiler.iarewarm.dlib" value="full"/>
		<config name="config.compiler.iarewarm.create_aux" value="no"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<config name="config.compiler.as5_arm.create_aux" value="no"/>
		<device-support value="atsam4sd32c"/>
	</project>
</asf>
