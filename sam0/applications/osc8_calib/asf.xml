<asf xmlversion="1.0">
	<module type="application" id="sam0.applications.osc8_calib" caption="8MHz Oscillator Calibration Application">
		<info type="description" value="summary">
		Example application to calibrate osc8MHz clock source and set up usart for embedded debugger
		</info>
		<build type="c-source" value="osc8_calib.c"/>
		<build type="include-path" value="."/>
		<require idref="sam0.drivers.tc"/>
		<require idref="sam0.drivers.events"/>
		<require idref="sam0.drivers.port"/>
		<require idref="sam0.drivers.sercom.usart"/>
		<device-support value="samd20"/>
	</module>
</asf>
