<asf xmlversion="1.0">
	<module type="application" id="sam0.applications.tictactoe" caption="Tic Tac Toe Application">
		<info type="description" value="summary">
		Example application to play Tic Tac Toe on the OLED1 Xplained extension wing.
		</info>
		<info type="keyword" value="technology">
		</info>
		<info type="gui-flag" value="move-to-root"/>

		<build type="c-source" value="tictactoe.c"/>

		<build type="include-path" value="."/>

		<require idref="sam0.drivers.system"/>
		<require idref="common2.services.gfx_mono"/>
		<require idref="common2.services.gfx_mono.font"/>
		<require idref="common2.services.delay"/>
		<require idref="sam0.drivers.port"/>

		<device-support value="samd20"/>
	</module>
</asf>
