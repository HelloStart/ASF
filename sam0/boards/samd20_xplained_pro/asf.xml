<asf xmlversion="1.0">
	<select-by-config id="config.board.samd20_xplained_pro.led" name="config.board.samd20_xplained_pro.led" caption="SAMD20 Xplained PRO LED support" default="yes">
		<module type="meta" id="config.board.samd20_xplained_pro.led#no" caption="SAMD20 Xplained PRO LED support disabled">
		</module>
		<module type="meta" id="config.board.samd20_xplained_pro.led#yes" caption="SAMD20 Xplained PRO LED support enabled">
			<build type="c-source" value="led.c"/>
			<build type="header-file" subtype="api" value="led.h"/>
			<build type="include-path" value="."/>
			<require idref="sam0.drivers.port"/>
		</module>
	</select-by-config>

	<select-by-config id="config.board.samd20_xplained_pro.init" name="config.board.samd20_xplained_pro.init" caption="SAMD20 Xplained PRO initialization support" default="yes">
		<module type="meta" id="config.board.samd20_xplained_pro.init#no" caption="SAMD20 Xplained PRO initialization support disabled">
		</module>
		<module type="meta" id="config.board.samd20_xplained_pro.init#yes" caption="SAMD20 Xplained PRO initialization support enabled">
			<build type="c-source" value="init.c"/>
			<require idref="sam0.drivers.port"/>
		</module>
	</select-by-config>

	<board id="board.samd20_xplained_pro" vendor="Atmel" caption="SAMD20 Xplained PRO">
		<build type="define" name="BOARD" value="SAMD20_XPLAINED_PRO"/>
		<build type="define" name="__SAMD20J18A__" value=""/>
		<build type="header-file" value="samd20_xplained_pro.h"/>
		<build type="include-path" value="."/>
		<build type="include-path" value=".."/>
		<build type="module-config" subtype="required-header-file" value="conf_board.h"/>
		<build type="module-config" subtype="path" value="board_config"/>
		<build type="macro-config" subtype="script" value="debug_scripts/gcc/samd20_xplained_pro_flash.gdb" toolchain="armgcc"/>
		<build type="macro-config" subtype="aux-script" value="debug_scripts/gcc/samd20_xplained_pro_sram.gdb" toolchain="armgcc"/>
		<build type="macro-config" subtype="script" value="debug_scripts/iar/samd20_xplained_pro_flash.mac" toolchain="iarewarm"/>
		<build type="macro-config" subtype="aux-script" value="debug_scripts/iar/samd20_xplained_pro_sram.mac" toolchain="iarewarm"/>
		<require idref="sam0.utils.cmsis.samd20.source.template"/>
		<require idref="config.board.samd20_xplained_pro.init"/>
		<require idref="config.board.samd20_xplained_pro.led"/>
		<require idref="common.boards"/>
		<device-support value="samd20j18a"/>
	</board>
</asf>
