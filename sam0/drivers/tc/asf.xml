<asf xmlversion="1.0">
	<select-by-config caption="TC driver" default="polled" id="sam0.drivers.tc" name="sam0.drivers.tc">
		<module caption="TC Driver" id="sam0.drivers.tc#polled" type="driver">
			<info type="description" value="summary">
			  Driver for the SAMD20 Timer/Counter module. Provides a unified
			  interface for the configuration and management of the device's
			  hardware timers in polled mode.
			</info>
			<info type="keyword" value="technology">
				<keyword value="I/O" />
				<keyword value="Timers" />
				<keyword value="Clocks" />
			</info>
			<build subtype="example" type="header-file" value="quick_start/qs_tc_basic.h" />
			<build type="doxygen-entry-point" value="asfdoc_samd2_tc_group" />
			<require idref="sam0.utils" />
			<require idref="sam0.drivers.system" />
			<require idref="sam0.drivers.port" />
			<device-support value="samd20" />
			<build type="c-source" value="tc.c" />
			<build subtype="api" type="header-file" value="tc.h" />
			<build subtype="example" type="header-file" value="quick_start/qs_tc_basic.h" />
			<build type="include-path" value="." />
			<build type="doxygen-image-path" value="docimg" />
			<build type="doxygen-entry-point" value="asfdoc_samd20_tc_group" />
			<require idref="sam0.drivers.port" />
			<generator default-mcu="unspecified-samd20" value="doxygen-module-v2" />
		</module>
		<module caption="TC callback driven driver" id="sam0.drivers.tc#callback" type="driver">
			<info type="description" value="summary">
			  Driver for the SAMD20 Timer/Counter module. Provides a unified
			  interface for the configuration and management of the device's
			  hardware timers in callback and/or polled mode.
			</info>
			<build type="include-path" value="." />
			<build subtype="api" type="header-file" value="tc_interrupt.h" />
			<build type="c-source" value="tc_interrupt.c" />
			<build name="TC_ASYNC" type="define" value="" />
			<build type="doxygen-entry-point" value="asfdoc_samd20_tc_group" />
			<build type="doxygen-entry-point" value="asfdoc_samd20_tc_async_group" />
			<build subtype="example" type="header-file" value="quick_start_callback/qs_tc_callback.h" />
			<require idref="sam0.utils" />
			<require idref="sam0.drivers.system" />
			<require idref="sam0.drivers.port" />
			<device-support value="samd20" />
			<build type="c-source" value="tc.c" />
			<build subtype="api" type="header-file" value="tc.h" />
			<build subtype="example" type="header-file" value="quick_start/qs_tc_basic.h" />
			<build type="include-path" value="." />
			<build type="doxygen-image-path" value="docimg" />
			<build type="doxygen-entry-point" value="asfdoc_samd20_tc_group" />
			<generator default-mcu="unspecified-samd20" value="doxygen-module-v2" />
		</module>
	</select-by-config>
</asf>
