<asf xmlversion="1.0">
	<select-by-config caption="UC3-A3 Xplained LED support" default="yes" id="config.board.uc3_a3_xplained.led" name="config.board.uc3_a3_xplained.led">
		<module caption="UC3-A3 Xplained LED support disabled" id="config.board.uc3_a3_xplained.led#no" type="meta">
		</module>
		<module caption="UC3-A3 Xplained LED support enabled" id="config.board.uc3_a3_xplained.led#yes" type="meta">
			<build type="c-source" value="led.c" />
			<build type="include-path" value="." />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<select-by-config caption="UC3-A3 Xplained initialization support" default="yes" id="config.board.uc3_a3_xplained.init" name="config.board.uc3_a3_xplained.init">
		<module caption="UC3-A3 Xplained initialization support disabled" id="config.board.uc3_a3_xplained.init#no" type="meta">
		</module>
		<module caption="UC3-A3 Xplained initialization support enabled" id="config.board.uc3_a3_xplained.init#yes" type="meta">
			<build subtype="required-header-file" type="module-config" value="conf_board.h" />
			<build subtype="path" type="module-config" value="board_config" />
			<build type="c-source" value="init.c" />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<board caption="UC3-A3 Xplained" id="board.uc3_a3_xplained" vendor="Atmel">
		<build name="BOARD" type="define" value="UC3_A3_XPLAINED" />
		<build type="header-file" value="uc3_a3_xplained.h" />
		<build subtype="api" type="header-file" value="led.h" />
		<build type="include-path" value="." />
		<build type="include-path" value=".." />
		<require idref="common.boards" />
		<require idref="config.board.uc3_a3_xplained.led" />
		<require idref="config.board.uc3_a3_xplained.init" />
		<config name="config.compiler.avr32gcc.programming_clock" value="int" />
		<device-support value="at32uc3a3256" />
	</board>
</asf>