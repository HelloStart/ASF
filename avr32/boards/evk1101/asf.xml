<asf xmlversion="1.0">
	<select-by-config caption="EVK1101 LED support" default="yes" id="config.board.evk1101.led" name="config.board.evk1101.led">
		<module caption="EVK1101 LED support disabled" id="config.board.evk1101.led#no" type="meta">
		</module>
		<module caption="EVK1101 LED support enabled" id="config.board.evk1101.led#yes" type="meta">
			<build type="c-source" value="led.c" />
			<build type="include-path" value="." />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<select-by-config caption="EVK1101 initialization support" default="yes" id="config.board.evk1101.init" name="config.board.evk1101.init">
		<module caption="EVK1101 initialization support disabled" id="config.board.evk1101.init#no" type="meta">

		</module>
		<module caption="EVK1101 initialization support enabled" id="config.board.evk1101.init#yes" type="meta">
			<build subtype="required-header-file" type="module-config" value="conf_board.h" />
			<build subtype="path" type="module-config" value="board_config" />
			<build type="c-source" value="init.c" />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<board caption="EVK1101" id="board.evk1101" vendor="Atmel">
		<build name="BOARD" type="define" value="EVK1101" />
		<build type="header-file" value="evk1101.h" />
		<build subtype="api" type="header-file" value="led.h" />
		<build type="include-path" value="." />
		<build type="include-path" value=".." />
		<require idref="common.boards" />
		<require idref="config.board.evk1101.led" />
		<require idref="config.board.evk1101.init" />
		<device-support value="at32uc3b0256" />
	</board>
</asf>