<asf xmlversion="1.0">
	<select-by-config caption="EVK1100 LED support" default="yes" id="config.board.evk1100.led" name="config.board.evk1100.led">
		<module caption="EVK1100 LED support disabled" id="config.board.evk1100.led#no" type="meta">
		</module>
		<module caption="EVK1100 LED support enabled" id="config.board.evk1100.led#yes" type="meta">
			<build type="c-source" value="led.c" />
			<build type="include-path" value="." />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<select-by-config caption="EVK1100 initialization support" default="yes" id="config.board.evk1100.init" name="config.board.evk1100.init">
		<module caption="EVK1100 initialization support disabled" id="config.board.evk1100.init#no" type="meta">
		</module>
		<module caption="EVK1100 initialization support enabled" id="config.board.evk1100.init#yes" type="meta">
			<build subtype="required-header-file" type="module-config" value="conf_board.h" />
			<build subtype="path" type="module-config" value="board_config" />
			<build type="c-source" value="init.c" />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<board caption="EVK1100" id="board.evk1100" vendor="Atmel">
		<build name="BOARD" type="define" value="EVK1100" />
		<build type="header-file" value="evk1100.h" />
		<build subtype="api" type="header-file" value="led.h" />
		<build type="include-path" value="." />
		<build type="include-path" value=".." />
		<require idref="common.boards" />
		<require idref="config.board.evk1100.led" />
		<require idref="config.board.evk1100.init" />
		<device-support value="at32uc3a0512" />
	</board>
</asf>