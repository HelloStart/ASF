<asf xmlversion="1.0">
	<select-by-config caption="RZ600 LED support" default="yes" id="config.board.rz600.led" name="config.board.rz600.led">
		<module caption="RZ600 LED support disabled" id="config.board.rz600.led#no" type="meta">

		</module>
		<module caption="RZ600 LED support enabled" id="config.board.rz600.led#yes" type="meta">
			<build type="c-source" value="led.c" />
			<build type="include-path" value="." />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<select-by-config caption="RZ600 initialization support" default="yes" id="config.board.rz600.init" name="config.board.rz600.init">
		<module caption="RZ600 initialization support disabled" id="config.board.rz600.init#no" type="meta">

		</module>
		<module caption="RZ600 initialization support enabled" id="config.board.rz600.init#yes" type="meta">
			<build subtype="required-header-file" type="module-config" value="conf_board.h" />
			<build subtype="path" type="module-config" value="board_config" />
			<build type="c-source" value="init.c" />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<board caption="RZ600S" id="board.rz600s" vendor="Atmel">
		<build name="BOARD" type="define" value="RZ600" />
		<build type="header-file" value="rz600.h" />
		<build subtype="api" type="header-file" value="led.h" />
		<build type="include-path" value="." />
		<build type="include-path" value=".." />
		<require idref="common.boards" />
		<require idref="config.board.rz600.led" />
		<require idref="config.board.rz600.init" />
		<device-support value="at32uc3a3256s" />
	</board>
</asf>