<asf xmlversion="1.0">
	<select-by-config caption="STK600-RCUC3L3 LED support" default="yes" id="config.board.stk600-rcuc3l3.led" name="config.board.stk600-rcuc3l3.led">
		<module caption="STK600-RCUC3L3 LED support disabled" id="config.board.stk600-rcuc3l3.led#no" type="meta">
		</module>
		<module caption="STK600-RCUC3L3 LED support enabled" id="config.board.stk600-rcuc3l3.led#yes" type="meta">
			<build type="include-path" value="." />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<select-by-config caption="STK600-RCUC3L3 initialization support" default="yes" id="config.board.stk600-rcuc3l3.init" name="config.board.stk600-rcuc3l3.init">
		<module caption="STK600-RCUC3L3 initialization support disabled" id="config.board.stk600-rcuc3l3.init#no" type="meta">
		</module>
		<module caption="STK600-RCUC3L3 initialization support enabled" id="config.board.stk600-rcuc3l3.init#yes" type="meta">
			<build subtype="required-header-file" type="module-config" value="conf_board.h" />
			<build subtype="path" type="module-config" value="board_config" />
			<build type="c-source" value="init.c" />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<board caption="STK600-RCUC3L3" id="board.stk600-rcuc3l3" vendor="Atmel">
		<build name="BOARD" type="define" value="STK600_RCUC3L3" />
		<build type="header-file" value="stk600_rcuc3l3.h" />
		<build subtype="api" type="header-file" value="led.h" />
		<build type="include-path" value="." />
		<build type="include-path" value="../.." />
		<build name="__ATUC256L3U__" toolchain="iarew32" type="define" value="" />
		<require idref="common.boards" />
		<require idref="config.board.stk600-rcuc3l3.led" />
		<require idref="config.board.stk600-rcuc3l3.init" />
		<config name="config.compiler.avr32gcc.programming_clock" value="int" />
		<device-support value="atuc256l3u" />
	</board>
</asf>