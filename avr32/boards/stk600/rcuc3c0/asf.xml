<asf xmlversion="1.0">
	<select-by-config caption="STK600-RCUC3C0 LED support" default="yes" id="config.board.stk600-rcuc3c0.led" name="config.board.stk600-rcuc3c0.led">
		<module caption="STK600-RCUC3C0 LED support disabled" id="config.board.stk600-rcuc3c0.led#no" type="meta">
		</module>
		<module caption="STK600-RCUC3C0 LED support enabled" id="config.board.stk600-rcuc3c0.led#yes" type="meta">
			<build type="c-source" value="led.c" />
			<build type="include-path" value="." />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<select-by-config caption="STK600-RCUC3C0 initialization" default="yes" id="config.board.stk600-rcuc3c0.init" name="config.board.stk600-rcuc3c0.init">
		<module caption="STK600-RCUC3C0 initialization support disabled" id="config.board.stk600-rcuc3c0.init#no" type="meta">

		</module>
		<module caption="STK600-RCUC3C0 initialization support enabled" id="config.board.stk600-rcuc3c0.init#yes" type="meta">
			<build subtype="required-header-file" type="module-config" value="conf_board.h" />
			<build subtype="path" type="module-config" value="board_config" />
			<build type="c-source" value="init.c" />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<board caption="STK600" id="board.stk600-rcuc3c0" vendor="Atmel">
		<build name="BOARD" type="define" value="STK600_RCUC3C0" />
		<build type="header-file" value="stk600_rcuc3c0.h" />
		<build subtype="api" type="header-file" value="led.h" />
		<build type="include-path" value="." />
		<build type="include-path" value="../.." />
		<require idref="common.boards" />
		<require idref="config.board.stk600-rcuc3c0.led" />
		<require idref="config.board.stk600-rcuc3c0.init" />
		<config name="config.compiler.avr32gcc.programming_clock" value="int" />
		<device-support value="at32uc3c0512c" />
	</board>
</asf>