<asf xmlversion="1.0">
	<select-by-config caption="STK600-RCUC3D LED support" default="yes" id="config.board.stk600-rcuc3d.led" name="config.board.stk600-rcuc3d.led">
		<module caption="STK600-RCUC3D LED support disabled" id="config.board.stk600-rcuc3d.led#no" type="meta">
		</module>
		<module caption="STK600-RCUC3D LED support enabled" id="config.board.stk600-rcuc3d.led#yes" type="meta">
			<build type="c-source" value="led.c" />
			<build type="include-path" value="." />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<select-by-config caption="STK600-RCUC3D initialization support" default="yes" id="config.board.stk600-rcuc3d.init" name="config.board.stk600-rcuc3d.init">
		<module caption="STK600-RCUC3D initialization support disabled" id="config.board.stk600-rcuc3d.init#no" type="meta">
		</module>
		<module caption="STK600-RCUC3D initialization support enabled" id="config.board.stk600-rcuc3d.init#yes" type="meta">
			<build subtype="required-header-file" type="module-config" value="conf_board.h" />
			<build subtype="path" type="module-config" value="board_config" />
			<build type="c-source" value="init.c" />
			<require idref="avr32.drivers.gpio" />
		</module>
	</select-by-config>

	<board caption="STK600" id="board.stk600-rcuc3d" vendor="Atmel">
		<build name="BOARD" type="define" value="STK600_RCUC3D" />
		<build name="__ATUC128D3__" toolchain="iarew32" type="define" value="" />
		<build type="header-file" value="stk600_rcuc3d.h" />
		<build subtype="api" type="header-file" value="led.h" />
		<build type="include-path" value="." />
		<build type="include-path" value="../.." />
		<require idref="common.boards" />
		<require idref="config.board.stk600-rcuc3d.led" />
		<require idref="config.board.stk600-rcuc3d.init" />
		<config name="config.compiler.avr32gcc.programming_clock" value="int" />
		<device-support value="atuc128d3" />
	</board>
</asf>