<asf xmlversion="1.0">
	<select-by-config caption="XMEGA-A3BU Xplained LED support" default="yes" id="config.board.xmega_a3bu_xplained.led" name="config.board.xmega_a3bu_xplained.led">
		<module caption="XMEGA-A3BU Xplained LED support disabled" id="config.board.xmega_a3bu_xplained.led#no" type="meta">
		</module>
		<module caption="XMEGA-A3BU Xplained LED support enabled" id="config.board.xmega_a3bu_xplained.led#yes" type="meta">
			<build subtype="api" type="header-file" value="led.h" />
			<build type="include-path" value="." />
			<require idref="common.services.basic.gpio" />
			<require idref="xmega.drivers.ioport" />
		</module>
	</select-by-config>

	<select-by-config caption="XMEGA-A3BU Xplained initialization support" default="yes" id="config.board.xmega_a3bu_xplained.init" name="config.board.xmega_a3bu_xplained.init">
		<module caption="XMEGA-A3BU Xplained initialization support disabled" id="config.board.xmega_a3bu_xplained.init#no" type="meta">
		</module>
		<module caption="XMEGA-A3BU Xplained initialization support enabled" id="config.board.xmega_a3bu_xplained.init#yes" type="meta">
			<build type="c-source" value="init.c" />
			<require idref="common.services.basic.gpio" />
			<require idref="xmega.drivers.ioport" />
		</module>
	</select-by-config>

	<board caption="XMEGA-A3BU Xplained" id="board.xmega_a3bu_xplained" vendor="Atmel">
		<build name="BOARD" type="define" value="XMEGA_A3BU_XPLAINED" />
		<build subtype="required-header-file" type="module-config" value="conf_board.h" />
		<build subtype="path" type="module-config" value="board_config" />
		<build type="header-file" value="xmega_a3bu_xplained.h" />
		<build type="include-path" value="." />
		<build type="include-path" value=".." />
		<build name="CONFIG_NVM_IGNORE_XMEGA_A3_D3_REVB_ERRATA" type="define" value="" />
		<build subtype="flags" toolchain="avrgcc" type="linker-config" value="--section-start=.BOOT=0x40000" />
		<require idref="common.boards" />
		<require idref="config.board.xmega_a3bu_xplained.led" />
		<require idref="config.board.xmega_a3bu_xplained.init" />
		<config name="config.spi_master_type" value="usart_and_standard_spi" />
		<config name="config.gfx_mono.display" value="c12832_a1z" />
		<device-support value="atxmega256a3bu" />
	</board>
</asf>