<asf xmlversion="1.0">
	<module type="application" id="xmega.drivers.tc.example1" caption="TC Example 1">
		<info type="description" value="summary">
		Use the XMEGA Timer Counter module : configures one TC module in normal mode and uses three TC interrupts (overflow, Capture/Compare Channels A and B).
		</info>
		<info type="keyword" value="technology">
			<keyword value="Timers"/>
		</info>
		<build type="c-source" value="tc_example1.c"/>
		<build type="c-source" value="common_sw_timer.c"/>
		<build type="header-file" value="common_sw_timer.h"/>
		<build type="c-source" value="common_hw_timer.c"/>
		<build type="header-file" value="common_hw_timer.h"/>
		<build type="header-file" value="avrtypes.h"/>
		<build type="header-file" value="mcu_types.h"/>
		<build type="include-path" value="."/>
		<build type="module-config" subtype="required-header-file" value="conf_example.h"/>
		<build type="module-config" subtype="required-header-file" value="conf_common_hw_timer.h"/>
		<device-support value="xmega"/>
		<require idref="common.services.basic.gpio"/>
		<require idref="xmega.drivers.tc"/>
	</module>
	<module type="application" id="xmega.drivers.tc.hwexample1" caption="TC hw Example 1">
		<info type="description" value="summary">
		Use the XMEGA Timer Counter module : configures one TC module in normal mode and uses three TC interrupts (overflow, Capture/Compare Channels A and B).
		</info>
		<info type="keyword" value="technology">
			<keyword value="Timers"/>
		</info>
		<build type="c-source" value="tc_hw_example1.c"/>
		<build type="c-source" value="common_hw_timer.c"/>
		<build type="header-file" value="common_hw_timer.h"/>
		<build type="header-file" value="avrtypes.h"/>
		<build type="header-file" value="mcu_types.h"/>
		<build type="include-path" value="."/>
		<build type="module-config" subtype="required-header-file" value="conf_example.h"/>
		<build type="module-config" subtype="required-header-file" value="conf_common_hw_timer.h"/>
		<device-support value="xmega"/>
		<require idref="common.services.basic.gpio"/>
		<require idref="xmega.drivers.tc"/>
	</module>
</asf>
