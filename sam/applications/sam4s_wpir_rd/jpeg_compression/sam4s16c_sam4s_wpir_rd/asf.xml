<asf xmlversion="1.0">
	<project caption="jpeg compression exemple for SAM4S_WPIR_RD" id="sam.applications.jpeg_compression.sam4s16c_sam4s_wpir_rd">
		<info type="description" value="summary">
		This exemple show how to use jpeg libraries to compress and decompress a capturing image on sam4s_wpir_rd board
		</info>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_ili9325.h"/>
		<build type="header-file" value="conf_aat31xx.h"/>
		<build type="header-file" value="conf_uart_serial.h"/>
		<build type="include-path" value="."/>
		<config name="config.compiler.iarewarm.heap_size" value="0xA000"/>
		<config name="config.compiler.armgcc.heap_size" value="0xA000"/>
		<config name="config.compiler.as5_arm.heap_size" value="0xA000"/>
		<config name="config.compiler.iarewarm.create_aux" value="no"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<config name="config.compiler.as5_arm.create_aux" value="no"/>
		<require idref="sam.applications.jpeg_compression"/>
		<require idref="thirdparty.libjpeg.port_ijg_8d"/>			
		<require idref="sam.utils.syscalls"/>
		<require idref="board.sam4s_wpir_rd"/>
		<generator value="iarewarm"/>
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<generator value="doxygen"/>
	</project>
</asf>
