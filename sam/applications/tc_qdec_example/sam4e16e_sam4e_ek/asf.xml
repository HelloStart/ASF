<asf xmlversion="1.0">
	<project caption="TC Quadrature Decoder Example on SAM4E-EK" id="sam.applications.tc_qdec_example.sam4e_ek"> 
		<info type="description" value="summary">
		This example shows how to configure TC in Quadrature Decoder mode with different feature like 
		position and Rotation measurement, Speed measurement, Direction status and change detection.
		</info>
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_uart_serial.h"/>
		<build type="header-file" value="conf_example.h"/>
		<build type="include-path" value="."/>
		<config name="config.common2.services.delay" value="cycle"/>
		<require idref="sam.applications.tc_qdec_example"/>
		<require idref="board.sam4e_ek"/>
		<generator value="iarewarm"/>
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<device-support value="atsam4e16e"/>
	</project>
</asf>

