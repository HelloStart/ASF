<asf xmlversion="1.0">
	<module type="application" id="sam.applications.sam4l_qtouch_demo" caption="SAM4L Low Power and QTouch Demo">
		<info type="description" value="summary">
		SAM4L is running with either LCD and QTouch library enabled or in low power sleep modes.
		</info>
		<build type="c-source" value="main.c"/>
		<build type="c-source" value="gui.c"/>
		<build type="header-file" subtype="api" value="gui.h"/>
		<build type="c-source" value="app.c"/>
		<build type="header-file" subtype="api" value="app.h"/>
		<build type="include-path" value="."/>
		<build type="c-source" value="qtouch/touch.c"/>
		<build type="header-file" subtype="api" value="qtouch/QDebugSettings.h"/>
		<build type="header-file" subtype="api" value="qtouch/touch_config_sam4l.h"/>
		<build type="include-path" value="qtouch/"/>
		<require idref="sam.drivers.usart"/>
		<build type="c-source" value="src/ast.c"/>
		<build type="c-source" value="src/c42364a.c"/>
		<build type="c-source" value="src/eic.c"/>
		<build type="c-source" value="src/lcdca.c"/>
		<build type="c-source" value="src/pdca.c"/>
		<build type="c-source" value="src/bscif.c"/>
		<build type="header-file" subtype="api" value="src/ast.h"/>
		<build type="header-file" subtype="api" value="src/c42364a.h"/>
		<build type="header-file" subtype="api" value="src/eic.h"/>
		<build type="header-file" subtype="api" value="src/lcdca.h"/>
		<build type="header-file" subtype="api" value="src/pdca.h"/>
		<build type="header-file" subtype="api" value="src/conf_lcdca.h"/>
		<build type="header-file" subtype="api" value="src/bscif.h"/>
		<build type="include-path" value="./src/"/>
		<require idref="common.services.basic.clock"/>
		<require idref="common.services.basic.sleepmgr"/>
		<require idref="common.utils.stdio"/>
		<require idref="config.common.services.qtouch.qdebug.qt600.sam4l" />
		<require idref="sam.services.qtouch.devspecific.sam.sam4l" />
		<device-support value="sam4l"/>
	</module>
</asf>
