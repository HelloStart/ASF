<asf xmlversion="1.0">
	<board id="board.sam4s_xplained_pro" vendor="Atmel" caption="SAM4S Xplained PRO">
		<device-support value="atsam4sd32c"/>
		<build type="define" name="BOARD" value="SAM4S_XPLAINED_PRO"/>
		<build type="include-path" value="."/>
		<build type="include-path" value=".."/>
		<build type="c-source" value="board_init.c"/>
		<build type="header-file" value="sam4s_xplained_pro.h"/>
		<build type="header-file" subtype="api" value="board_io.h"/>
		<build type="module-config" subtype="required-header-file" value="conf_board.h"/>
		<build type="module-config" subtype="path" value="board_config"/>
		<build type="macro-config" subtype="script" value="debug_scripts/gcc/sam4s_xplained_pro_flash.gdb"
			 toolchain="armgcc"/>
		<build type="macro-config" subtype="aux-script" value="debug_scripts/gcc/sam4s_xplained_pro_sram.gdb"
			 toolchain="armgcc"/>
		<build type="macro-config" subtype="script" value="debug_scripts/iar/sam4s_xplained_pro_flash.mac"
			 toolchain="iarewarm"/>
		<build type="macro-config" subtype="aux-script" value="debug_scripts/iar/sam4s_xplained_pro_sram.mac"
			 toolchain="iarewarm"/>
		<require idref="common.utils"/>
		<require idref="common.boards"/>
		<require idref="common.services.ioport"/>
		<require idref="sam.utils.cmsis.sam4s.source.template"/>
	</board>
</asf>
