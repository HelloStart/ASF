<asf xmlversion="1.0">
	<select-by-config caption="SAM4L-EK LED support" default="no" id="config.board.sam4l_ek.led" name="config.board.sam4l_ek.led">
		<module caption="SAM4L-EK LED support disabled" id="config.board.sam4l_ek.led#no" type="meta">
		</module>
		<module caption="SAM4L-EK LED support enabled" id="config.board.sam4l_ek.led#yes" type="meta">
			<build subtype="api" type="header-file" value="led.h" />
			<build type="include-path" value="." />
			<require idref="common.services.ioport" />
		</module>
	</select-by-config>

	<select-by-config caption="SAM4L-EK initialization support" default="yes" id="config.board.sam4l_ek.init" name="config.board.sam4l_ek.init">
		<module caption="SAM4L-EK initialization support disabled" id="config.board.sam4l_ek.init#no" type="meta">
		</module>
		<module caption="SAM4L-EK initialization support enabled" id="config.board.sam4l_ek.init#yes" type="meta">
			<build type="c-source" value="init.c" />
			<require idref="common.services.ioport" />
		</module>
	</select-by-config>

	<select-by-config caption="SAM4L-EK Board Monitor support" default="no" id="config.board.sam4l_ek.bm" name="config.board.sam4l_ek.bm">
		<module caption="SAM4L-EK Board Monitor support disabled" id="config.board.sam4l_ek.bm#no" type="meta">
		</module>
		<module caption="SAM4L-EK Board Monitor support enabled" id="config.board.sam4l_ek.bm#yes" type="meta">
			<build type="c-source" value="board_monitor.c" />
			<build type="header-file" value="board_monitor.h" />
			<build type="include-path" value="." />
			<require idref="sam.drivers.usart" />
		</module>
	</select-by-config>

	<board caption="SAM4L-EK" id="board.sam4l_ek">
		<build name="BOARD" type="define" value="SAM4L_EK" />
		<build name="__ATSAM4LC4C__" type="define" value="" />
		<build type="header-file" value="sam4l_ek.h" />
		<build type="include-path" value="." />
		<build type="include-path" value=".." />
		<build subtype="script" toolchain="iarewarm" type="macro-config" value="debug_scripts/iar/sam4l_ek_flash.mac" />
		<build subtype="script" toolchain="armgcc" type="macro-config" value="debug_scripts/gcc/sam4l_ek_flash.gdb" />
		<build subtype="aux-script" toolchain="armgcc" type="macro-config" value="debug_scripts/gcc/sam4l_ek_sram.gdb" />
		<require idref="sam.utils.cmsis.sam4l.source.template" />
		<require idref="common.boards" />
		<require idref="config.board.sam4l_ek.init" />
		<require idref="config.board.sam4l_ek.led" />
		<require idref="config.board.sam4l_ek.bm" />
		<device-support value="atsam4lc4c" />
	</board>
</asf>