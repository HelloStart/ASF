<asf xmlversion="1.0">
	<select-by-config caption="SAM3S EK LED support" default="yes" id="config.board.sam3s_ek2.led" name="config.board.sam3s_ek2.led">
		<module caption="SAM3S EK2 LED support disabled" id="config.board.sam3s_ek2.led#no" type="meta">
		</module>
		<module caption="SAM3S EK2 LED support enabled" id="config.board.sam3s_ek2.led#yes" type="meta">
			<build type="c-source" value="led.c" />
			<build subtype="api" type="header-file" value="led.h" />
			<build type="include-path" value="." />
			<require idref="common.services.basic.gpio" />
			<require idref="sam.drivers.pio" />
		</module>
	</select-by-config>

	<select-by-config caption="SAM3S initialization support" default="yes" id="config.board.sam3s_ek2.init" name="config.board.sam3s_ek2.init">
		<module caption="SAM3S initialization support disabled" id="config.board.sam3s_ek2.init#no" type="meta">
		</module>
		<module caption="SAM3S initialization support enabled" id="config.board.sam3s_ek2.init#yes" type="meta">
			<build type="c-source" value="init.c" />
			<require idref="common.services.basic.gpio" />
			<require idref="sam.drivers.pio" />
		</module>
	</select-by-config>

	<board caption="SAM3S-EK2" id="board.sam3s_ek2" vendor="Atmel">
		<build name="BOARD" type="define" value="SAM3S_EK2" />
		<build name="__SAM3SD8C__" type="define" value="" />
		<build type="header-file" value="sam3s_ek2.h" />
		<build type="include-path" value="." />
		<build type="include-path" value=".." />
		<build subtype="required-header-file" type="module-config" value="conf_board.h" />
		<build subtype="path" type="module-config" value="board_config" />
		<build subtype="script" toolchain="armgcc" type="macro-config" value="debug_scripts/gcc/sam3s_ek2_flash.gdb" />
		<build subtype="aux-script" toolchain="armgcc" type="macro-config" value="debug_scripts/gcc/sam3s_ek2_sram.gdb" />
		<build subtype="script" toolchain="iarewarm" type="macro-config" value="debug_scripts/iar/sam3s_ek2_flash.mac" />
		<build subtype="aux-script" toolchain="iarewarm" type="macro-config" value="debug_scripts/iar/sam3s_ek2_sram.mac" />
		<require idref="sam.utils.cmsis.sam3sd8.source.template" />
		<require idref="common.boards" />
		<require idref="config.board.sam3s_ek2.init" />
		<require idref="config.board.sam3s_ek2.led" />
		<device-support value="atsam3sd8c" />
	</board>
</asf>