<asf xmlversion="1.0">
	<select-by-config id="config.board.sam4l_xplained_pro.led" name="config.board.sam4l_xplained_pro.led" caption="SAM4L-XPLAINED-PRO LED support" default="yes">
		<module type="meta" id="config.board.sam4l_xplained_pro.led#no" caption="SAM4L-XPLAINED-PRO LED support disabled">
		</module>
		<module type="meta" id="config.board.sam4l_xplained_pro.led#yes" caption="SAM4L-XPLAINED-PRO LED support enabled">
			<build type="header-file" subtype="api" value="led.h"/>
			<build type="include-path" value="."/>
			<require idref="common.services.ioport"/>
		</module>
	</select-by-config>

	<select-by-config id="config.board.sam4l_xplained_pro.init" name="config.board.sam4l_xplained_pro.init" caption="SAM4L-XPLAINED-PRO initialization support" default="yes">
		<module type="meta" id="config.board.sam4l_xplained_pro.init#no" caption="SAM4L-XPLAINED-PRO initialization support disabled">
		</module>
		<module type="meta" id="config.board.sam4l_xplained_pro.init#yes" caption="SAM4L-XPLAINED-PRO initialization support enabled">
			<build type="c-source" value="init.c"/>
			<require idref="common.services.ioport"/>
		</module>
	</select-by-config>

	<board id="board.sam4l_xplained_pro" vendor="Atmel" caption="SAM4L-XPLAINED-PRO"> 
		<build type="define" name="BOARD" value="SAM4L_XPLAINED_PRO"/>
		<build type="define" name="__ATSAM4LC4C__" value=""/>
		<build type="header-file" value="sam4l_xplained_pro.h"/>
		<build type="include-path" value="."/>
		<build type="include-path" value=".."/>
		<build type="module-config" subtype="required-header-file" value="conf_board.h"/>
		<build type="module-config" subtype="path" value="board_config"/>
		<build type="macro-config" subtype="script" value="debug_scripts/iar/sam4l_xplained_pro_flash.mac" toolchain="iarewarm"/>
		<build type="macro-config" subtype="script" value="debug_scripts/gcc/sam4l_xplained_pro_flash.gdb" toolchain="armgcc"/>
		<build type="macro-config" subtype="aux-script" value="debug_scripts/gcc/sam4l_xplained_pro_sram.gdb" toolchain="armgcc"/>
		<require idref="sam.utils.cmsis.sam4l.source.template"/>
		<require idref="common.boards"/>
		<require idref="config.board.sam4l_xplained_pro.init"/>
		<require idref="config.board.sam4l_xplained_pro.led"/>
		<device-support value="atsam4lc4c"/>
	</board>
</asf>
