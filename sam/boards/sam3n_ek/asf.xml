<asf xmlversion="1.0">
	<select-by-config caption="SAM3N EK LED support" default="yes" id="config.board.sam3n_ek.led" name="config.board.sam3n_ek.led">
		<module caption="SAM3N EK LED support disabled" id="config.board.sam3n_ek.led#no" type="meta">
		</module>
		<module caption="SAM3N EK LED support enabled" id="config.board.sam3n_ek.led#yes" type="meta">
			<build type="c-source" value="led.c" />
			<build subtype="api" type="header-file" value="led.h" />
			<build type="include-path" value="." />
			<require idref="common.services.basic.gpio" />
			<require idref="sam.drivers.pio" />
		</module>
	</select-by-config>

	<select-by-config caption="SAM3N initialization support" default="yes" id="config.board.sam3n_ek.init" name="config.board.sam3n_ek.init">
		<module caption="SAM3N initialization support disabled" id="config.board.sam3n_ek.init#no" type="meta">
		</module>
		<module caption="SAM3N initialization support enabled" id="config.board.sam3n_ek.init#yes" type="meta">
			<build type="c-source" value="init.c" />
			<require idref="common.services.basic.gpio" />
			<require idref="sam.drivers.pio" />
		</module>
	</select-by-config>

	<board caption="SAM3N-EK" id="board.sam3n_ek" vendor="Atmel">
		<build name="BOARD" type="define" value="SAM3N_EK" />
		<build name="__SAM3N4C__" type="define" value="" />
		<build type="header-file" value="sam3n_ek.h" />
		<build type="include-path" value="." />
		<build type="include-path" value=".." />
		<build subtype="required-header-file" type="module-config" value="conf_board.h" />
		<build subtype="path" type="module-config" value="board_config" />
		<build subtype="script" toolchain="armgcc" type="macro-config" value="debug_scripts/gcc/sam3n_ek_flash.gdb" />
		<build subtype="aux-script" toolchain="armgcc" type="macro-config" value="debug_scripts/gcc/sam3n_ek_sram.gdb" />
		<build subtype="script" toolchain="iarewarm" type="macro-config" value="debug_scripts/iar/sam3n_ek_flash.mac" />
		<build subtype="aux-script" toolchain="iarewarm" type="macro-config" value="debug_scripts/iar/sam3n_ek_sram.mac" />
		<require idref="sam.utils.cmsis.sam3n.source.template" />
		<require idref="common.boards" />
		<require idref="config.board.sam3n_ek.init" />
		<require idref="config.board.sam3n_ek.led" />
		<device-support value="atsam3n4c" />
	</board>
</asf>