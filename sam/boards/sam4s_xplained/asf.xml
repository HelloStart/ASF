<asf xmlversion="1.0">
	<select-by-config caption="SAM4S Xplained LED support" default="yes" id="config.board.sam4s_xplained.led" name="config.board.sam4s_xplained.led">
		<module caption="SAM4S Xplained LED support disabled" id="config.board.sam4s_xplained.led#no" type="meta">
		</module>
		<module caption="SAM4S Xplained LED support enabled" id="config.board.sam4s_xplained.led#yes" type="meta">
			<build type="c-source" value="led.c" />
			<build subtype="api" type="header-file" value="led.h" />
			<build type="include-path" value="." />
			<require idref="common.services.basic.gpio" />
			<require idref="sam.drivers.pio" />
		</module>
	</select-by-config>

	<select-by-config caption="SAM4S Xplained initialization support" default="yes" id="config.board.sam4s_xplained.init" name="config.board.sam4s_xplained.init">
		<module caption="SAM4S Xplained initialization support disabled" id="config.board.sam4s_xplained.init#no" type="meta">
		</module>
		<module caption="SAM4S Xplained initialization support enabled" id="config.board.sam4s_xplained.init#yes" type="meta">
			<build type="c-source" value="init.c" />
			<require idref="common.services.basic.gpio" />
			<require idref="sam.drivers.pio" />
			<require idref="sam.drivers.pmc" />
		</module>
	</select-by-config>

	<board caption="SAM4S Xplained" id="board.sam4s_xplained" vendor="Atmel">
		<build name="BOARD" type="define" value="SAM4S_XPLAINED" />
		<build name="__SAM4S16C__" type="define" value="" />
		<build type="header-file" value="sam4s_xplained.h" />
		<build type="include-path" value="." />
		<build type="include-path" value=".." />
		<build subtype="required-header-file" type="module-config" value="conf_board.h" />
		<build subtype="path" type="module-config" value="board_config" />
		<build subtype="script" toolchain="armgcc" type="macro-config" value="debug_scripts/gcc/sam4s_xplained_flash.gdb" />
		<build subtype="aux-script" toolchain="armgcc" type="macro-config" value="debug_scripts/gcc/sam4s_xplained_sram.gdb" />
		<build subtype="script" toolchain="iarewarm" type="macro-config" value="debug_scripts/iar/sam4s_xplained_flash.mac" />
		<build subtype="aux-script" toolchain="iarewarm" type="macro-config" value="debug_scripts/iar/sam4s_xplained_sram.mac" />
		<require idref="sam.utils.cmsis.sam4s.source.template" />
		<require idref="common.boards" />
		<require idref="config.board.sam4s_xplained.init" />
		<require idref="config.board.sam4s_xplained.led" />
		<device-support value="atsam4s16c" />
	</board>
</asf>