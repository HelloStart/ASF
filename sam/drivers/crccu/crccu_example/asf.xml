<asf xmlversion="1.0">
	<module type="application" id="sam.drivers.crccu.crccu_example" caption="CRCCU Example">
		<info type="description" value="summary">
		This example demonstrates the Cyclic Redundancy Check Calculation Unit (CRCCU) provided on SAM3 microcontrollers. It shows how to use CRCCU to compute CRC on a memory area.
		</info>
		<build type="c-source" value="crccu_example.c"/>
		<build type="include-path" value="."/>
		<device-support value="sam"/>
		<require idref="sam.drivers.crccu"/>
		<require idref="sam.drivers.pmc"/>
		<require idref="sam.drivers.pio"/>
		<require idref="sam.drivers.uart"/>
		<require idref="sam.services.flash"/>
		<require idref="common.services.basic.gpio"/>
		<require idref="common.services.basic.clock"/>
		<require idref="common.utils.stdio.stdio_serial"/>
	</module>
</asf>
